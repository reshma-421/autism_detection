	<script>
        window.addEventListener('DOMContentLoaded', () => {
            if (document.body.clientWidth > 1000) {
                SmoothScroll({
                    animationTime: 1000, // [ms]
                    stepSize: 100, // [px]
                    accelerationDelta: 20,
                    accelerationMax: 5,
                    keyboardSupport: true,
                    arrowScroll: 50, // [px]
                    touchpadSupport: false,
                });
            }

            $('.reviews-slider').slick({
                infinite: true,
                slidesToShow: 1,
                slidesToScroll: 1,
                speed: 500,
                fade: true,
                cssEase: 'linear',
                arrows: false,
                asNavFor: '.reviews-slider-name'
            });

            $('.reviews-slider-name').slick({
                infinite: true,
                slidesToShow: 3,
                slidesToScroll: 1,
                vertical: true,
                focusOnSelect: true,
                asNavFor: '.reviews-slider',
                arrows: false,
                responsive: [
                    {
                        breakpoint: 1028,
                        settings: {
                            vertical: false,
                        }
                    },
                ]
            });

            if (window.screen.width <= 540) {

                function initSliderCart(slider) {
                    $(slider).slick({
                        infinite: true,
                        centerMode: true,
                        variableWidth: true,
                        arrows: false
                    });
                }

                initSliderCart('.slider-cart-health')
                initSliderCart('.slider-cart-chilld')
                initSliderCart('.slider-cart-blog')
                initSliderCart('.narrow-block__cart-block--resources')
            }


            function sliderReveiwsLineRemove() {
                const parent = document.querySelector('.reviews-slider-name__wrp')

                if (parent) {
                    let lineArray = parent.querySelectorAll('.reviews-slider-name__line')
                    lineArray.forEach((line, index) => {
                        console.log(index)
                        if (lineArray.length - 1 == index) {
                            line.remove()
                        }
                    })
                }
            }

            sliderReveiwsLineRemove()
        })
    </script>

