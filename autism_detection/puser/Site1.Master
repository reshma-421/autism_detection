	<script>

        window.addEventListener("DOMContentLoaded", () => {
            SmoothScroll({
                animationTime: 1000, // [ms]
                stepSize: 100, // [px]
                accelerationDelta: 20,
                accelerationMax: 5,
                keyboardSupport: true,
                arrowScroll: 50, // [px]
                touchpadSupport: false,
            });

            function bodyOpenMenu() {
                const btnMenu = document.querySelector('.btn-burger ')

                btnMenu.addEventListener('click', () => {
                    document.body.classList.toggle('open-menu')
                })

            }
            bodyOpenMenu()

            $(".header__logo, .nav__link, .btn, .footer__list-nav_item_link, .footer__logo-wrp, .cart-blog, .hero-service__link, .all-resoureces__drop-list_link, .get-started__link").click(function () {

                var link = $(this).attr("href");


                if (link != undefined) {
                    if (link.length >= 3) {
                        $('.crt').get(0).click()
                        setTimeout(function () {
                            window.location.href = link;
                        }, 1200);
                        return false;
                    } else if (link == '/') {
                        $('.crt').get(0).click()
                        setTimeout(function () {
                            window.location.href = link;
                        }, 1200);
                        return false;
                    }
                }


            });
        })

    </script>
